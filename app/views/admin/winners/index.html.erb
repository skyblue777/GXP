<h1 class="page-header">
  Winners (Contest: <%= @contest %>)
</h1>
<% if @projects.count > 0 %>
  <table class="table table-striped table-contests">
    <thead>
      <tr>
        <th>Project name</th>
        <th>Vote count</th>
        <th class="text-right">Actions</th>
      </tr>
    </thead>
    <tbody>
    <%= render collection: @projects, partial: 'admin/winners/project' %>
    </tbody>
  </table>
<% else %>
  <tr>
    <td colspan=3 style="text-align:center;">No Projects posted for the contest.</td>
  </tr>
<% end %>
