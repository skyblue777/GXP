<h1 class="page-header">
  Projects
  <%#= link_to 'New', new_admin_project_path, class: 'btn btn-primary' %>
</h1>
<table class="table table-striped table-projects">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>User</th>
      <th>Group?</th>
      <th>Contest</th>
      <th>Category</th>
      <th>Created At</th>
      <th class="text-right">Actions</th>
    </tr>
  </thead>
  <tbody>
    <%= render @projects %>
  </tbody>
</table>
